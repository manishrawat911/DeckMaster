<table style="border: 1px solid;">
	<tr style="border: 1px solid;">
	[%for (col in table.columns) { %]
    	<th style="border: 1px solid;">[%=col.name%]</th>
	[%}%]
	</tr>
	[%for (row in table.rows) { %]
    <tr style="border: 1px solid;">
    [%for (cell in row.cells) { %]		
        <td style="border: 1px solid;">[%=cell.value%]</td>
	[%}%]
    </tr>
	[%}%]
</table>